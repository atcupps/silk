{
    "manifest_version": 3,
    "name": "Terrafin",
    "version": "1.0",
    "permissions": ["scripting"],
    "web_accessible_resources": [
        {
          "resources": ["popup.css"],
          "matches": ["<all_urls>"]
        }
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["popup.js"],
        "run_at": "document_idle"
      }
    ]
  }
  